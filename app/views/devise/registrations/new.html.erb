<h2>Crear Cuenta</h2>

<%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
  <%= render "devise/shared/error_messages", resource: resource %>

  <div class="field">
    <%= f.label :correo %><br />
    <%= f.email_field :email, autofocus: true, autocomplete: "correo" %>
  </div>

  <div class="field">
    <%= f.label :socio %><br />
    <%= f.text_field :socio, autocomplete: "nombre usuario" %>
  </div>

  <div class="field">
    <%= f.label :contrase単a %>
    <% if @minimum_password_length %>
    <em>(<%= @minimum_password_length %> characters minimum)</em>
    <% end %><br />
    <%= f.password_field :password, autocomplete: "nueva contrase単a" %>
  </div>

  <div class="field">
    <%= f.label :confirmar_contrase単a %><br />
    <%= f.password_field :password_confirmation, autocomplete: "confirmar_contrase単a" %>
  </div>

  <div class="actions">
    <%= f.submit "Registrarse" %>
  </div>
<% end %>

<%= render "devise/shared/links" %>
